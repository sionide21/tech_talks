<h3><%= @video.title %></h3>
<div id="players"></div>

<%= link "Back", to: video_path(@conn, :show, @video) %>

<script>
  onInit(function() {
    let Presenter = require("web/static/scripts/presenter").default;
    let playerDiv = document.getElementById("players");
    new Presenter(playerDiv, {session: "<%= @session %>", video: "<%= video_id(@video) %>"});
  });
</script>
